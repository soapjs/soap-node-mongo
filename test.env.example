# MongoDB Integration Test Configuration
# Copy this file to .env and adjust values for your environment

# MongoDB Test Connection
MONGO_TEST_URL=mongodb://localhost:27017
MONGO_TEST_DB=soapjs_test

# Optional: MongoDB Authentication (if needed)
# MONGO_TEST_USER=testuser
# MONGO_TEST_PASSWORD=testpass
# MONGO_TEST_AUTH_SOURCE=admin

# Optional: MongoDB SSL (if needed)
# MONGO_TEST_SSL=true

# Test Timeouts (in milliseconds)
MONGO_TEST_TIMEOUT=30000
